{
  "family": "narrativa3",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "narrativa3",  
      "image": "AWS_ACCOUNT_ID.dkr.ecr.AWS_REGION.amazonaws.com/testest:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "ecsTaskExecutionRole"
}
